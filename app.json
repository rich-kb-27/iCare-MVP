{
  "expo": {
    "name": "iCare",
    "slug": "icare-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "icare",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.richkb27.icare",
      "infoPlist": {
        "NSCameraUsageDescription": "iCare needs camera access for video consultations.",
        "NSMicrophoneUsageDescription": "iCare needs microphone access for video consultations."
      }
    },
    "android": {
      "package": "com.richkb27.icare",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#0F172A"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icare-logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#0F172A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f780d3c7-6b6d-443e-961b-6fd028b09b4d"
      }
    }
  }
}
